<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="site.metacoding.testproject.domain.notice.NoticeDao">

	<insert id="insert">

	</insert>

	<select id="findAll"
		resultType="site.metacoding.testproject.domain.notice.Notice">

	</select>

	<select id="findById"
		resultType="site.metacoding.testproject.domain.notice.Notice">

	</select>

	<select id="findSubsByEmployeeId"
		resultType="site.metacoding.testproject.domain.notice.Notice">
		SELECT *
		FROM notice n
		INNER JOIN subscribe s
		ON n.company_id = s.company_id
		WHERE s.employee_id = #{employeeId}
	</select>

	<update id="update">

	</update>

	<delete id="deleteById">

	</delete>


</mapper>