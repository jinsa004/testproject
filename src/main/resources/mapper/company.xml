<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="site.metacoding.testproject.domain.company.CompanyDao">

	<insert id="insert"></insert>

	<select id="findAll" resultType="site.metacoding.testproject.domain.company.Company">
		SELECT company_id, row_number() over(order BY company_id desc) as NO,
		company_number, company_name, company_email, company_tel, company_location, job_id, company_username, company_password
		from company
	</select>

	<select id="findById" resultType="site.metacoding.testproject.domain.company.Company"></select>

	<update id="update"></update>

	<delete id="deleteById"></delete>


</mapper>